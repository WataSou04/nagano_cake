<div class="col-lg-2">
  <div>
    <table class="table">
      <tr><th>ジャンル検索</th></tr>
      <%= form_with url: items_path, method: :get do |f| %>
        <% genres.each do |genre| %>
        <tr><td><%= link_to genre.name, items_path(genre_name: genre.name), method: :get %></td></tr>
        <% end %>
      <% end %>
    </table>
  </div>
</div>