<h5>配送先編集</h5>

<%= form_with model: @shippingaddress, url: shipping_address_path, method: :patch do |f| %>
<table>
  <tr><td><label for="Postal">郵便番号(ハイフンなし)</label></td><td><%= f.text_field :postal_code, autofocus: true, id:"Postal" %></td></tr>
  <tr><td><label for="Address">住所</label></td><td><%= f.text_field :address, autofocus: true, id:"Address" %></td></tr>
  <tr><td><label for="Name">宛名</label></td><td><%= f.text_field :name, autofocus: true, id:"Name" %></td><td><%= f.submit '変更内容を保存', class: 'btn btn-success' %></td></tr>
</table>
<% end %>